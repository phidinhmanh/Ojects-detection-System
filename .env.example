# Environment Configuration
# Copy to .env and customize

# API Configuration
API_HOST=0.0.0.0
API_PORT=8080
API_DEBUG=false
CORS_ORIGINS=*

# Model Configuration
MODEL_ARCH=mobilenetv2_ssd
MODEL_INPUT_SIZE=320
MODEL_NUM_CLASSES=80
BATCH_SIZE=16
EPOCHS=50
LEARNING_RATE=0.001

# MLflow Configuration
MLFLOW_TRACKING_URI=mlruns
MLFLOW_EXPERIMENT=object-detection

# Paths (relative to project root)
DATA_DIR=data
RAW_DIR=data/raw
PROCESSED_DIR=data/processed
MODEL_DIR=model
REPORTS_DIR=reports

# Database (optional)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=object_detection
DB_USER=postgres
DB_PASSWORD=
